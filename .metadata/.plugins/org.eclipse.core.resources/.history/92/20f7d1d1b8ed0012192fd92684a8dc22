#include <windows.h>

// 実行ファイルにデータファイルなどをD&Dするとファイル名がフルパスでメッセージボックスに表示される
int WINAPI WinMain(HINSTANCE hInstance, HINSTANCE hPrevInstance, PSTR lpCmdLine, int nCmdShow)
{
	MessageBox(NULL, lpCmdLine, TEXT("Kitty"), MB_ICONINFORMATION);
	return 0;
}


